<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<!-- Font Awesome -->
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.11.2/css/all.css" />
	<!-- Google Fonts Roboto -->
	<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Roboto:wght@300;400;500;700&display=swap" />
	<!-- Bootstrap 5 -->
	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
		integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
	<title>Payroll New Codes</title>
</head>

<body>
	<div class="container">

		<div class="row">
			<div class="col-12 mt-3 mb-5 text-center">
				<h2 class="text-uppercase">Payroll New Codes</h2>
			</div>
		</div>

		<div class="row">


			<div class="row">
				<h5 class="text-uppercase col-sm-6 col-12">Candidate Main Info</h5>
				<h5 class="text-uppercase col-sm-6 col-12">Payslip</h5>
			</div>

			<div class="table-responsive col-sm-6 col-12">
				<table class="table table-bordered text-center">
					<thead>
						<tr class="table-info">
							<th>Candidate Info</th>
							<th>Value</th>
						</tr>
					</thead>
					<tbody>
						<% Object.keys(inputs.candidate).forEach(function(prop) { %>
						<tr>
							<td><%- prop %></td>
							<td><%- inputs.candidate[prop] %></td>
						</tr>
						<% }); %>
					</tbody>
				</table>
			</div>
			
			<div class="table-responsive col-sm-6 col-12">
				<table class="table table-bordered text-center">
					<thead>
						<tr class="table-info">
							<th>Company Deductions</th>
							<th>Value</th>
						</tr>
					</thead>
					<tbody>
						<% Object.keys(outputs).forEach(function(prop) { %>
						<tr>
							<td><%- prop %></td>
							<td><%- outputs[prop] %> &#163;</td>
						</tr>
						<% }); %>
					</tbody>
				</table>
			</div>
		</div>


		<div class="row">
			<h5 class="text-uppercase col-sm-6 col-12">Timesheets</h5>
		</div>

		<div class="row mt-3">
			<% inputs.timesheets.forEach(function(timesheet) { %>
				<div class="table-responsive col-12">
					<table class="table table-bordered text-center">
						<thead>
							<tr class="table-info">
								<th>Company Receipts</th>
								<th>Describtion</th>
								<th>Week End Date</th>
								<th>Units</th>
								<th>Rate</th>
								<th>Amount</th>
								<th>VAT</th>
							</tr>
						</thead>
						<tbody>
							<% timesheet.forEach(function(item) { %>
							<tr>
								<td><%- item.itemType %></td>
								<td><%- item.description %></td>
								<td><%- item.weekEndDate %></td>
								<td><%- item.unit %></td>
								<td><%- item.rate %></td>
								<td><%- item.amount %></td>
								<td><%- item.vat %></td>
							</tr>
							<% }); %>
						</tbody>
					</table>
				</div>
			<% }); %>

		</div>

	</div>

</body>

</html>